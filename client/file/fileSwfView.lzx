<library>
	<class name="fileSwfView" extends="window">
		<attribute name="im" type="string"/>
		<attribute name="resizable" value="true"/>
		<attribute name="closeable" value="true"/>
		<attribute name="clickable" value="true"/>
		<view resource="${classroot.im}" stretches="both" height="${classroot.height}" width="${classroot.width}"></view>
	</class>
</library>